{
	"name": "@nan0web/mail",
	"version": "1.0.0",
	"description": "Mailing with the nodemailer and nano format and many channels to deliver nicely formatted messages",
	"type": "module",
	"main": "./src/index.js",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"test": "node --test --test-timeout=9999 src/**/*.test.js | nan0test parse --fail",
		"clean": "rm -rf .cache/ && rm -rf dist/",
		"clean:modules": "rm -rf node_modules",
		"test:docs": "node --test src/README.md.js",
		"test:release": "node --test \"releases/**/*.test.js\"",
		"test:coverage": "node --experimental-test-coverage --test-coverage-include=\"src/**/*.js\" --test-coverage-exclude=\"src/**/*.test.js\" --test \"src/**/*.test.js\"",
		"test:coverage:collect": "nan0test coverage",
		"test:status": "nan0test status",
		"precommit": "npm test",
		"prepush": "npm test",
		"prepare": "husky",
		"play": "node play/main.js"
	},
	"files": [
		"src",
		"types"
	],
	"keywords": [
		"nano",
		"nan0",
		"nan0web",
		"mail",
		"email",
		"nodemailer",
		"node"
	],
	"author": "ЯRаСлав YaRaSLove <support@yaro.page>",
	"license": "ISC",
	"dependencies": {
		"nodemailer": "^6.10.1"
	},
	"devDependencies": {
		"@nan0web/db-fs": "^1.0.0",
		"@nan0web/html": "^0.2.0",
		"@nan0web/xml": "^1.0.0",
		"@nan0web/test": "^1.0.2",
		"husky": "^9.1.7"
	},
	"peerDependencies": {
		"@nan0web/db-fs": "^1.0.0",
		"@nan0web/html": "^0.2.0",
		"@nan0web/xml": "^1.0.0"
	}
}
